{
  "build": {
    "content": [
      {
        "src": "./",
        "files": [
          "**/*.md",
          "**/*.yml",
          "breadcrumb/toc.yml"
        ],
        "exclude": [
          "**/includes/**",
          "**/mref/**",
          "**/obj/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "src": "./",
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "markdownEngineName": "markdig",
    "globalMetadata": {
      "breadcrumb_path": "/azure-data-studio/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "permissioned-type": "public"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "azure-data-studio"
  }
}